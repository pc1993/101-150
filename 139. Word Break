class Solution {
public:
    bool wordBreak(string s, vector<string>& wordDict) {
        int n=s.size();
        if(wordDict.size()==0)
            return false;
        unordered_set<string> dict;
        dict.insert(wordDict.begin(),wordDict.end());
        vector<bool> dp(n+1,false);
        dp[0]=true;
        for(int i=1;i<=n;++i)
        {
            for(int j=i-1;j>=0;--j)
            {
                if(dp[j])
                {
                    string word=s.substr(j,i-j);
                    if(dict.find(word)!=dict.end())
                    {
                        dp[i]=true;
                        break;
                    }
                }
            }
        }
        return dp[n];
    }
};
